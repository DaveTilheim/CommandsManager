


function pow value power

	create i 0
	create powered value
	update powered 1
	while less i power
		update powered mul powered value
		update i add i 1
	end while

	return powered

end function


!suite de fibonacci en r√©cursif

function fib n
	
	if less n 0
		return null
	else
		if less n 2
			return n
		end
	end

	call fib sub n 1
	call fib sub n 2
	
	return add ret:0 ret

end function


